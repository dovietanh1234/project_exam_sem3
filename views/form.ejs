        <h1>ADD PROUCT</h1></br>
      
      
        ProductCode
        <div class="form-group">
          <input type="text" class="form-control" id="exampleInputEmail1" placeholder="ProductCode" name="ProductCode">
        </div>
      
      
        ProductName
        <div class="form-group">
          <input type="text" class="form-control" id="exampleInputEmail1" placeholder="ProductName" name="ProductName">
